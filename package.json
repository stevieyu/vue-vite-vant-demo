{
  "name": "vue-vite-vant-demo",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite --host 0.0.0.0",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "eslint --fix --ext .js,.jsx,.vue --format friendly",
    "surge": "cp dist/index.html dist/200.html && npx surge dist vvvd.surge.stevie.top",
    "presurge": "npm run build",
    "gh": "cp dist/index.html dist/404.html && npx -y gh-pages -d dist -b gh-pages",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@vueuse/components": "^8.9.1",
    "@vueuse/core": "^8.0.0",
    "loadjs": "^4.2.0",
    "pinia": "^2.0.6",
    "vant": "^4.0.0-0",
    "vue": "^3.2.19",
    "vue-request": "^2.0.0-0",
    "vue-router": "^4.0.11"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^3.0.0-beta.1",
    "@vitejs/plugin-vue-jsx": "^2.0.0-beta.0",
    "@vue/compiler-sfc": "^3.2.19",
    "unocss": "^0.43.0",
    "unplugin-vue-components": "^0.21.1",
    "vite": "^3.0.0",
    "vite-plugin-eslint": "^1.7.0",
    "vite-plugin-pages": "^0.25.0",
    "vite-plugin-vue-layouts": "^0.6.0"
  },
  "optionalDependencies": {
    "@commitlint/cli": "^17.0.0",
    "@commitlint/config-conventional": "^17.0.0",
    "eslint-config-google": "^0.14.0",
    "eslint-formatter-friendly": "^7.0.0",
    "eslint-plugin-vue": "^9.0.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.0"
  },
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "eslint --fix --format friendly"
    ]
  }
}
